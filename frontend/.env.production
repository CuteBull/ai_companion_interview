# Production environment variables for Multimodal Chat Frontend
# !!! IMPORTANT: Replace with your actual deployed backend URL !!!

# Backend API URL (point to your deployed backend)
VITE_API_BASE_URL=https://your-api.onrender.com

# Cloudinary configuration for frontend uploads
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Feature flags (optional)
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false